<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap>
	<!-- 根据搜索条件查询  -->
	<select id="findOccupationalEvaluateReceiptByMap">
		from OccupationalEvaluateReceipt t where t.delFlag = 0
		<isNotEmpty property="deptCodes">
			<![CDATA[and t.deptId in (:deptCodes)]]>
		</isNotEmpty>
		<isNotEmpty property="ids">
			<![CDATA[and t.id in (:ids)]]>
		</isNotEmpty>
		
		<isNotEmpty property="companyName">
			<![CDATA[and t.companyName like :companyName]]>
		</isNotEmpty>
		<isNotEmpty property="szzid">
			<![CDATA[and t.szzid = :szzid]]>
		</isNotEmpty>
		<isNotEmpty property="qyid">
			<![CDATA[and t.qyid = :qyid]]>
		</isNotEmpty>
		<isNotEmpty property="startTbrq">
			<![CDATA[and t.sendTime >= :startTbrq]]>
		</isNotEmpty>
		<isNotEmpty property="endTbrq">
			<![CDATA[and t.sendTime <= :endTbrq]]>
		</isNotEmpty>
		<isNotEmpty property="pjjg">
			<![CDATA[and t.pjjg = :pjjg]]>
		</isNotEmpty>
		<isNotEmpty property="whfl">
			<![CDATA[and t.whfl = :whfl]]>
		</isNotEmpty>
		order by t.createTime desc
	</select>
</sqlMap>
